// Random theme from github.com/JanDeDobbeleer/oh-my-posh/tree/main/themes
{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "foreground": "#fbf1c7",
          "type": "path"
        },
        {
          "properties": {
            "branch_icon": "\ue0a0 ",
            "branch_identical_icon": "\u25cf",
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "plain",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#689d6a",
          "type": "git",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#fe8019{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fb4934{{ end }}",
            "{{ if gt .Ahead 0 }}#d3869b{{ end }}",
            "{{ if gt .Behind 0 }}#d3869b{{ end }}"
          ]
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " \ue65e {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#83a598",
          "type": "go"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#458588",
          "type": "julia"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#fabd2f",
          "type": "python"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": " \uedaf {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#8ec07c",
          "type": "java"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "plain",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#cc241d",
          "type": "ruby"
        },
        {
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "plain",
          "template": " \uf0e7{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#d79921",
          "type": "azfunc"
        },
        {
          "properties": {
            "display_default": false
          },
          "style": "plain",
          "template": " \ue7ad {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} ",
          "type": "aws",
          "foreground_templates": [
            "{{if contains \"default\" .Profile}}#d79921{{end}}",
            "{{if contains \"jan\" .Profile}}#b16286{{end}}"
          ]
        },
        {
          "style": "plain",
          "template": " \uf0ad ",
          "foreground": "#fabd2f",
          "type": "root"
        },
        {
          "style": "plain",
          "template": "‚ù±",
          "foreground": "#b8bb26",
          "type": "text"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
